{
    "builds": [
        {
          "src": "studybud/wsgi.py",
          "use": "@vercel/python",
          "config": { "maxLambdaSize": "15mb", "runtime": "python3.12.3" }
        }],
        "routes": [
            {
              "src": "/(.*)",
              "dest": "studybud/wsgi.py"
            }
          ]
}